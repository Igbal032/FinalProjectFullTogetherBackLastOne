
<!DOCTYPE html>
<html lang="en">

<head>
    <!-- Required meta tags-->
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <meta name="description" content="au theme template">
    <meta name="author" content="Hau Nguyen">
    <meta name="keywords" content="au theme template">

    <!-- Title Page-->
    <title>Dashboard</title>

    <!-- Fontfaces CSS-->
    <link href="~/Areas/Admin/Template/css/font-face.css" rel="stylesheet" media="all">
    <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.8.2/css/all.css">
    <link href="~/Areas/Admin/Template/vendor/mdi-font/css/material-design-iconic-font.min.css" rel="stylesheet" media="all">

    <!-- Bootstrap CSS-->
    <link href="~/Areas/Admin/Template/vendor/bootstrap-4.1/bootstrap.min.css" rel="stylesheet" media="all">

    <!-- Vendor CSS-->
    <link href="~/Areas/Admin/Template/vendor/animsition/animsition.min.css" rel="stylesheet" media="all">
    <link href="~/Areas/Admin/Template/vendor/bootstrap-progressbar/bootstrap-progressbar-3.3.4.min.css" rel="stylesheet" media="all">
    <link href="~/Areas/Admin/Template/vendor/wow/animate.css" rel="stylesheet" media="all">
    <link href="~/Areas/Admin/Template/vendor/css-hamburgers/hamburgers.min.css" rel="stylesheet" media="all">
    <link href="~/Areas/Admin/Template/vendor/slick/slick.css" rel="stylesheet" media="all">
    <link href="~/Areas/Admin/Template/vendor/select2/select2.min.css" rel="stylesheet" media="all">
    <link href="~/Areas/Admin/Template/vendor/perfect-scrollbar/perfect-scrollbar.css" rel="stylesheet" media="all">

    <!-- Main CSS-->
    <link href="~/Areas/Admin/Template/css/theme.css" rel="stylesheet" media="all">
    <!-- Imgae-Hover -->
    <link rel="stylesheet" href="~/Template/css/imagehover.min.css">
     @RenderSection("css", required: false)
</head>

<body class="animsition">
    <header class="header-desktop">
        <div class="section__content section__content--p30">
            <div class="container-fluid">
                <div class="header-wrap">
                    <form class="form-header" action="" method="POST">
                        <input class="au-input au-input--xl" type="text" name="search" placeholder="Search for datas &amp; reports..." />
                        <button class="au-btn--submit" type="submit">
                            <i class="zmdi zmdi-search"></i>
                        </button>
                    </form>
                    <div class="header-button">
                        @Html.Partial("~/Areas/Admin/Views/Admin/messageIconAndnotificationIcon.cshtml")
                        <div class="account-wrap">
                          @Html.Action("accountPart", "Admin")
                            @*@Html.Partial("~/Areas/Admin/Views/Admin/accountPart.cshtml")*@

                        </div>
                    </div>
                </div>
            </div>
        </div>
    </header>
    <div class="page-wrapper">
        <!-- HEADER MOBILE-->
        <header class="header-mobile d-block d-lg-none">
            <div class="header-mobile__bar">
                <div class="container-fluid">
                    <div class="header-mobile-inner">
                        <a class="logo" href="index.html">
                            <img src="~/Areas/Admin/Template/images/icon/logo.png" alt="CoolAdmin" />
                        </a>
                        <button class="hamburger hamburger--slider" type="button">
                            <span class="hamburger-box">
                                <span class="hamburger-inner"></span>
                            </span>
                        </button>
                    </div>
                </div>
            </div>
            <nav class="navbar-mobile">
                <div class="container-fluid">
                    <ul class="navbar-mobile__list list-unstyled">
                        <li class="active has-sub">
                            <a class="js-arrow" href="@Url.Action("Index","Admin")">
                                <i class="fas fa-tachometer-alt"></i>Əsas Səhifə
                            </a>
                        </li>
                        <li class="active has-sub">
                            <a class="js-arrow" href="@Url.Action("Index","ContactUS")">
                                <i class="far fa-envelope"></i>Ismarıclar
                            </a>
                        </li>
                    </ul>
                </div>
            </nav>
        </header>
        <!-- END HEADER MOBILE-->
        <!-- MENU SIDEBAR-->
        <aside class="menu-sidebar d-none d-lg-block">
            <div class="logo">
                <a href="#">
                    <img src="~/Areas/Admin/Template/images/icon/logo.png" alt="Cool Admin" />
                </a>
            </div>
            @Html.Partial("~/Areas/Admin/Views/Shared/_SieBarAdmin.cshtml");

        </aside>
        <!-- END MENU SIDEBAR-->
        <!-- PAGE CONTAINER-->
        @RenderBody()

    </div>

    <!-- Jquery JS-->
    <script src="~/Areas/Admin/Template/vendor/jquery-3.2.1.min.js"></script>
    <!-- Bootstrap JS-->
    <script src="~/Areas/Admin/Template/vendor/bootstrap-4.1/popper.min.js"></script>
    <script src="~/Areas/Admin/Template/vendor/bootstrap-4.1/bootstrap.min.js"></script>
    <!-- Vendor JS       -->
    <script src="~/Areas/Admin/Template/vendor/slick/slick.min.js"></script>
    <script src="~/Areas/Admin/Template/vendor/wow/wow.min.js"></script>
    <script src="~/Areas/Admin/Template/vendor/animsition/animsition.min.js"></script>
    <script src="~/Areas/Admin/Template/vendor/bootstrap-progressbar/bootstrap-progressbar.min.js"></script>
    <script src="~/Areas/Admin/Template/vendor/counter-up/jquery.waypoints.min.js"></script>
    <script src="~/Areas/Admin/Template/vendor/counter-up/jquery.counterup.min.js"></script>
    <script src="~/Areas/Admin/Template/vendor/circle-progress/circle-progress.min.js"></script>
    <script src="~/Areas/Admin/Template/vendor/perfect-scrollbar/perfect-scrollbar.js"></script>
    <script src="~/Areas/Admin/Template/vendor/chartjs/Chart.bundle.min.js"></script>
    <script src="~/Areas/Admin/Template/vendor/select2/select2.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/sweetalert/2.1.2/sweetalert.min.js"></script>

    <!-- Main JS-->
    <script src="~/Areas/Admin/Template/js/main.js"></script>
    @RenderSection("scripts", required: false)

</body>

</html>
<!-- end document-->
